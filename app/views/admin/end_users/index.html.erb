<div class = "container">
<table class = "table table-hover">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>会員名</th>
      <th>登録状況</th>
      <th>登録日時</th>
    </tr>
  </thead>
  <tbody>
    <% @end_users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= link_to admin_end_user_path(user.id) do %>
      <%= user.name %>
      <% end %>
      </td>
      <td><%= user.is_valid ? "有効" : "退会" %></td>
      <td><%= user.created_at.strftime('%Y/%m/%d %H:%M') %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @end_users %>
</div>
